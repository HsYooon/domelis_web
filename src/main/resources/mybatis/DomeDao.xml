<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.domelist.dome.repository.DomeDao">
    <select id="test" resultType="DomeDto">
        <![CDATA[
        SELECT id
              ,name
              ,img
              ,url
              ,title
              ,category
          from t_domelist
        ]]>
    </select>
    <select id="todayProductList" resultType="DomeDto">
        SELECT id
             ,name
             ,img
             ,url
             ,title
             ,category
        from t_domelist
        order by reg_dttm desc, id desc
    </select>
</mapper>